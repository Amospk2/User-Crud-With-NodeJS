<!DOCTYPE html>
<html lang="en">

<%- include('./partials/head.ejs') %>

<body>
    <%- include('./partials/navbar.ejs') %>
    <div class="content">
        <% if(users.length > 0){ %> <% users.forEach(user => { %>
            <h3 class="title"> <%= user.firstName %> <%= user.lastName %> </h3>
            <p><%= user.email %></p>
            <br>
        <% }) %> <% } else { %>
            <p>Não existem usuários no sistema.</p>
        <% } %>
    </div>
</body>

</html>